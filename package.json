{
  "name": "@revoke.cash/whois",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:RevokeCash/whois.git",
  "author": "Rosco Kalis <roscokalis@gmail.com>",
  "license": "MIT",
  "scripts": {
    "update:tokens:erc20": "tsx ./scripts/tokens/update-erc20-tokens.ts",
    "update:tokens:nft": "tsx ./scripts/tokens/update-nft-tokens.ts",
    "update:tokens:spam": "tsx ./scripts/tokens/update-spam-tokens.ts",
    "update:tokens:manual": "tsx ./scripts/tokens/copy-manual-tokens.ts",
    "update:tokens": "yarn update:tokens:erc20 && yarn update:tokens:spam && yarn update:tokens:manual",
    "update:spenders:ethereum-lists": "tsx ./scripts/spenders/update-ethereum-lists-spenders.ts",
    "update:spenders:uniswap": "tsx ./scripts/spenders/update-uniswap-spenders.ts",
    "update:spenders:risk-factors": "tsx ./scripts/spenders/update-risk-factors.ts",
    "update:spenders:universal": "tsx ./scripts/spenders/update-universal-spenders.ts",
    "update:spenders:manual": "tsx ./scripts/spenders/copy-manual-spenders.ts",
    "update:spenders": "yarn update:spenders:ethereum-lists && yarn update:spenders:uniswap && yarn update:spenders:risk-factors && yarn update:spenders:universal && yarn update:spenders:manual",
    "update:all": "yarn update:tokens && yarn update:spenders && yarn lint",
    "upload": "tsx ./scripts/storage/upload.ts",
    "lint:checksums": "tsx ./scripts/lint-checksums.ts",
    "lint:prettier": "prettier -uw .",
    "lint": "yarn lint:prettier && yarn lint:checksums",
    "postinstall": "git config --local include.path ../.gitconfig"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@revoke.cash/chains": "^63.0.0",
    "@types/node": "^20.11.26",
    "dotenv": "^16.4.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "p-queue": "^6.6.2",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.4.2",
    "viem": "^1.21.4",
    "walkdir": "^0.4.1"
  },
  "packageManager": "yarn@4.6.0"
}
